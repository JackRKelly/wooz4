{
	"name": "jrkdigital",
	"version": "0.1.0",
	"private": true,
	"main": "./src/pages/index.tsx",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint ./src/ --fix --no-cache",
		"lint:check": "next lint ./src/ --no-cache",
		"format": "yarn prettier --write ./src/**/*.{js,jsx,json,md,ts,tsx}",
		"format:check": "yarn prettier ./src/**/*.{js,jsx,json,md,ts,tsx}",
		"codegen": "yarn download-schema && apollo client:codegen --target typescript --globalTypesFile=src/@types/graphql-global.ts @types",
		"download-schema": "rover graph introspect https://wooz4.myshopify.com/api/graphql --header=\"X-Shopify-Storefront-Access-Token: a372aab299a287bb7c1aa9bb2eb0196b\" > src/graphql/schema.graphql"
	},
	"dependencies": {
		"@apollo/client": "^3.4.16",
		"@next/eslint-plugin-next": "11.1.3-canary.7",
		"graphql": "^15.7.1",
		"next": "^12.0.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"shopify-buy": "^2.12.0"
	},
	"devDependencies": {
		"@apollo/rover": "^0.3.0",
		"@babel/core": "^7.15.5",
		"@types/node": "^16.9.2",
		"@types/react": "^17.0.21",
		"@types/react-dom": "^17.0.9",
		"@types/shopify-buy": "^2.10.8",
		"@types/styled-components": "^5.1.14",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "7.32.0",
		"eslint-config-next": "11.1.3-canary.7",
		"eslint-config-xo": "^0.38.0",
		"eslint-config-xo-react": "^0.25.0",
		"eslint-config-xo-typescript": "^0.44.0",
		"eslint-plugin-react": "^7.25.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^7.0.2",
		"lint-staged": "^11.1.2",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1",
		"sass": "^1.41.1",
		"styled-components": "^5.3.1",
		"typescript": "^4.4.3",
		"webpack": "^5.60.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"lint-staged": {
		"*.js": "eslint",
		"*.ts": "eslint",
		"*.jsx": "eslint",
		"*.tsx": "eslint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && pretty-quick --staged"
		}
	}
}
